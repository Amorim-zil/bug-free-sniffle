-- Função para verificar o jogo atual
local function CheckGame()
    if table.find(GameID, game.PlaceId) then
        Rayfield:Notify({
            Title = "Jogo Detectado",
            Content = "O jogo foi detectado com sucesso!",
            Duration = 5,
            Image = 4483362458,
            Actions = {
                Ignore = {
                    Name = "Entendido",
                    Callback = function()
                        print("Jogo confirmado.")
                    end
                }
            }
        })
    else
        Rayfield:Notify({
            Title = "Jogo Não Detectado",
            Content = "Este jogo não é compatível.",
            Duration = 5,
            Image = 4483362458,
            Actions = {
                Ignore = {
                    Name = "Sair",
                    Callback = function()
                        game:Shutdown()
                    end
                }
            }
        })
    end
end

-- Função para definir a sorte no jogo Anime Rarities
local function SetLuck()
    if game.PlaceId == 16778527574 then
        local player = game.Players.LocalPlayer
        if player then
            -- Supondo que "luck" seja uma variável ou método no jogo
            local luckValue = 100 -- Define a sorte para 100x
            -- Substitua isso pela maneira real de modificar a sorte em Anime Rarities
            player.Luck = luckValue
            Rayfield:Notify({
                Title = "Sorte Definida",
                Content = "Sorte foi ajustada para 100x!",
                Duration = 5,
                Image = 4483362458,
                Actions = {
                    Ignore = {
                        Name = "Entendido",
                        Callback = function()
                            print("Sorte ajustada.")
                        end
                    }
                }
            })
        end
    end
end

